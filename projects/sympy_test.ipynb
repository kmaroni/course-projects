    "import sympy as sp\n",
    "x,i = sp.symbols('x,i')\n",
    "n = 5\n",
    "c = sp.cos(x).series(x,0,n)\n",
    "s = sp.sin(x).series(x,0,n)\n",
    "\n",
    "display(c)\n",
    "display(s)\n"
   ]
